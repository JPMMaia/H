{
    "name": "Export_and_import_c_header",
    "version": "0.1.0",
    "type": "library",
    "dependencies": [],
    "sources": [
        {
            "type": "hlang",
            "include": [
                "./**/*.hltxt"
            ]
        },
        {
            "type": "export_c_header",
            "include": [
                "./module_a.hltxt"
            ]
        },
        {
            "type": "import_c_header",
            "headers": [
                {
                    "name": "my_library.module_b",
                    "header": "module_b.h",
                    "dependencies": [
                        "my_library.module_a"
                    ]
                }
            ],
            "public_prefixes": [
                "my_library_module_b_"
            ],
            "remove_prefixes": [
                "my_library_module_b_"
            ]
        }
    ]
}
