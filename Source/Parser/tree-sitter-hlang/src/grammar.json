{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hlang",
  "rules": {
    "Module": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "Module_head"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "Declaration"
          }
        }
      ]
    },
    "Module_head": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "Module_declaration"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "Import"
          }
        }
      ]
    },
    "Module_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "Comment"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "module"
        },
        {
          "type": "SYMBOL",
          "name": "Module_name"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "Module_name": {
      "type": "SYMBOL",
      "name": "Identifier_with_dots"
    },
    "Import": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "import"
        },
        {
          "type": "SYMBOL",
          "name": "Import_name"
        },
        {
          "type": "STRING",
          "value": "as"
        },
        {
          "type": "SYMBOL",
          "name": "Import_alias"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "Import_name": {
      "type": "SYMBOL",
      "name": "Identifier_with_dots"
    },
    "Import_alias": {
      "type": "SYMBOL",
      "name": "Identifier"
    },
    "Declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "Comment"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "export"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "Struct"
            }
          ]
        }
      ]
    },
    "Type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "Builtin_type"
        },
        {
          "type": "SYMBOL",
          "name": "Type_name"
        }
      ]
    },
    "Type_name": {
      "type": "SYMBOL",
      "name": "Identifier"
    },
    "Builtin_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Float16"
        },
        {
          "type": "STRING",
          "value": "Float32"
        },
        {
          "type": "STRING",
          "value": "Float64"
        },
        {
          "type": "STRING",
          "value": "Bool"
        },
        {
          "type": "STRING",
          "value": "String"
        },
        {
          "type": "SYMBOL",
          "name": "Integer_type"
        }
      ]
    },
    "Integer_type": {
      "type": "PATTERN",
      "value": "(Int|Uint)([1-9]|[1-5][0-9]|6[0-4])"
    },
    "Struct": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "struct"
        },
        {
          "type": "SYMBOL",
          "name": "Struct_name"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "Struct_member"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "Struct_name": {
      "type": "SYMBOL",
      "name": "Identifier"
    },
    "Struct_member": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "Comment"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "Struct_member_name"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "Struct_member_type"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "Generic_expression_or_instantiate"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "Struct_member_name": {
      "type": "SYMBOL",
      "name": "Identifier"
    },
    "Struct_member_type": {
      "type": "SYMBOL",
      "name": "Type"
    },
    "Generic_expression_or_instantiate": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "Generic_expression"
        },
        {
          "type": "SYMBOL",
          "name": "Expression_instantiate"
        }
      ]
    },
    "Generic_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "Expression_constant"
        }
      ]
    },
    "Expression_constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "Boolean"
        },
        {
          "type": "SYMBOL",
          "name": "Number"
        },
        {
          "type": "SYMBOL",
          "name": "String"
        }
      ]
    },
    "Expression_instantiate": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{}"
        }
      ]
    },
    "Identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+"
    },
    "Identifier_with_dots": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "Identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "Identifier"
              }
            ]
          }
        }
      ]
    },
    "Boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "Number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "String": {
      "type": "PATTERN",
      "value": "\".*\""
    },
    "Comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}